{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:first",
          "foreground": "p:white",
          "leading_diamond": "",
          "powerline_symbol": "",
          "properties": {
            "macos": "mac"
          },
          "style": "diamond",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:second",
          "foreground": "p:white",
          "powerline_symbol": "",
          "style": "diamond",
          "template": " {{ if .SSHSession }} {{ end }}{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "p:third",
          "foreground": "p:white",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "PS"
            }
          },
          "style": "diamond",
          "template": " {{ .Name }} ",
          "type": "shell"
        },
        {
          "background": "p:forth",
          "foreground": "p:white",
          "powerline_symbol": "",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:fifth",
          "foreground": "p:white",
          "powerline_symbol": "",
          "properties": {
            "branch_icon": " ",
            "cherry_pick_icon": " ",
            "commit_icon": " ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": " ",
            "no_commits_icon": " ",
            "rebase_icon": " ",
            "revert_icon": " ",
            "tag_icon": " "
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:sixth",
          "foreground": "p:white",
          "powerline_symbol": "",
          "properties": {
            "always_enabled": true,
            "style": "austin",
            "threshold": 500
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }}",
          "trailing_diamond": "",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "palette": {
    "first": "#F72585",
    "second":"#B5179E",
    "third": "#7209B7",
    "forth": "#560BAD",
    "fifth": "#480CA8",
    "sixth": "#3F37C9",
    "white": "#FFFFFF"
  },
  "version": 2
}
