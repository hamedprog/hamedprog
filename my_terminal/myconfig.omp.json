{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#03071E",
          "foreground": "p:wight",
          "leading_diamond": "",
          "powerline_symbol": "",
          "properties": {
            "macos": "mac"
          },
          "style": "diamond",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#370617",
          "foreground": "p:wight",
          "powerline_symbol": "",
          "style": "diamond",
          "template": " {{ if .SSHSession }} {{ end }}{{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#6A040F",
          "foreground": "p:wight",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "PS"
            }
          },
          "style": "diamond",
          "template": " {{ .Name }} ",
          "type": "shell"
        },
        {
          "background": "#9D0208",
          "foreground": "p:wight",
          "powerline_symbol": "",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#D00000",
          "foreground": "p:wight",
          "powerline_symbol": "",
          "properties": {
            "branch_icon": " ",
            "cherry_pick_icon": " ",
            "commit_icon": " ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": " ",
            "no_commits_icon": " ",
            "rebase_icon": " ",
            "revert_icon": " ",
            "tag_icon": " "
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#DC2F02",
          "foreground": "p:wight",
          "powerline_symbol": "",
          "properties": {
            "always_enabled": true,
            "style": "austin",
            "threshold": 500
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }}",
          "trailing_diamond": "",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "palette": {
    "blue": "#8AADF4",
    "lavender": "#B7BDF8",
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "teal_blue": "#33658A",
    "text": "#494D64",
    "white": "#FFFFFF",
    "wight": "#FFFFFF"
  },
  "version": 2
}
